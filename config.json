            "type": "text"
        },
        {
            "key": "db-user",
            "name": "<b>DB User</b>",
            "required": true,
            "type": "text"
        },
        {
            "key": "db-password",
            "name": "<b>DB Password</b>",
            "required": true,
            "type": "text"
